# Redis configuration
REDIS_URL=redis://<your_redis_host>:<your_redis_port>

# Origin API configuration
ORIGIN_URL=http://<your_origin_host>:<your_origin_port>

# Cache settings
CACHE_DEFAULT_TTL=<default_ttl_in_seconds>
L1_CACHE_MAXSIZE=<max_cache_size>
TTL_BY_CONTENT_TYPE={
    "application/json": <ttl_for_json>,
    "image/png": <ttl_for_png>,
    "text/html": <ttl_for_html>
}
TTL_BY_PATH_PATTERN=[
    {"pattern": "/health", "ttl": <ttl_for_health_endpoint>},
    {"pattern": "/static/*", "ttl": <ttl_for_static_content>}
]
